  lemma fixed_func_col_16_eq_unpacked_of_lt_usable_rows (c: ValidCircuit P P_Prime) (n: ℕ) (hn: n < c.usable_rows):
    fixed_func_col_16 c n = if n < 256 then n else 0 := by
      split
      case isTrue h => exact fixed_func_col_16_0_to_255_unpacked c ⟨n, h⟩
      case isFalse h =>
        unfold fixed_func_col_16
        rewrite [
          if_neg (by omega),
          if_neg (by omega),
          if_neg (by omega),
          if_neg (by omega),
          if_pos (by omega)
        ]
        simp

